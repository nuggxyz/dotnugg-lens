{
    "name": "dotnugg-lens",
    "productName": "dotnuggLens",
    "homepage": ".",
    "version": "0.2.1-beta.1",
    "description": "dotnugg desktop application",
    "main": "index.js",
    "author": "@nuggxyz",
    "license": "MIT",
    "private": true,
    "scripts": {
        "test": "ts-node --project ./webpack/tsconfig.json webpack/scripts/test.ts",
        "start": "NODE_ENV=development ts-node --project ./webpack/tsconfig.json  webpack/scripts/start.ts",
        "build": "NODE_OPTIONS='--experimental-modules' ts-node --project ./webpack/tsconfig.json  webpack/scripts/build.ts",
        "serve": "serve -s build",
        "typechain": "typechain --target ethers-v5 --out-dir src/typechain/ './src/abis/*'",
        "dist": "yarn build && NODE_OPTIONS='--experimental-modules'  electron-builder",
        "dev": "concurrently -k \"BROWSER=none yarn start\" \"npm:electron\"",
        "electron": "wait-on tcp:3000 && NODE_ENV=development electron ."
    },
    "build": {
        "extends": null,
        "productName": "dotnuggLens",
        "appId": "com.nuggxyz.dotnuggLens",
        "icon": "./AppIcon/AppIcon.icns",
        "copyright": "Copyright Â© 2022 @nuggxyz",
        "files": [
            "./build/**/*",
            "./index.js",
            "./preload.js"
        ],
        "extraResources": [
            "./aseprite2dotnugg.lua",
            "./collection.nugg",
            "./lensDefault/**/*.nugg"
        ],
        "mac": {
            "artifactName": "${productName}-${version}.${ext}",
            "category": "public.app-category.productivity",
            "target": [
                "dmg"
            ]
        },
        "linux": {
            "artifactName": "${productName}-${version}-${arch}.${ext}",
            "maintainer": "xxxx",
            "target": [
                "tar.gz",
                "deb",
                "rpm"
            ]
        },
        "win": {
            "target": [
                "nsis"
            ]
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
