{
    "name": "dotnugg-lens",
    "productName": "dotnuggLens",
    "homepage": ".",
    "version": "0.0.2",
    "description": "dotnugg desktop application",
    "main": "index.js",
    "author": "@nuggxyz",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "node scripts/start.js",
        "develop": "node scripts/start.js",
        "build": "node scripts/build.js",
        "serve": "serve -s build",
        "test": "node scripts/test.js",
        "typechain": "typechain --target ethers-v5 --out-dir src/typechain/ './src/abis/*'",
        "dist": "yarn build && electron-builder",
        "dev": "concurrently -k \"BROWSER=none yarn start\" \"npm:electron\"",
        "electron": "wait-on tcp:3000 && electron ."
    },
    "build": {
        "extends": null,
        "productName": "dotnuggLens",
        "appId": "com.nuggxyz.dotnuggLens",
        "icon": "./AppIcon/AppIcon.icns",
        "files": [
            "./build/**/*",
            "./index.js",
            "./preload.js"
        ],
        "mac": {
            "artifactName": "${productName}-${version}.${ext}",
            "category": "public.app-category.productivity",
            "target": [
                "dmg",
                "zip"
            ]
        },
        "linux": {
            "artifactName": "${productName}-${version}-${arch}.${ext}",
            "maintainer": "xxxx",
            "target": [
                "tar.gz",
                "deb",
                "rpm"
            ]
        },
        "win": {
            "target": [
                "zip",
                "dir"
            ]
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.17.0",
        "@babel/eslint-parser": "^7.14.9",
        "@babel/plugin-proposal-decorators": "^7.17.0",
        "@babel/plugin-transform-typescript": "^7.16.8",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.14.5",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
        "@svgr/webpack": "^6.2.1",
        "@typechain/ethers-v5": "^9.0.0",
        "@types/bluebird": "^3.5.36",
        "@types/chai": "^4.2.21",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.4.10",
        "@types/react-helmet": "^6.1.4",
        "@types/ua-parser-js": "^0.7.36",
        "@types/websocket": "^1.0.4",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
